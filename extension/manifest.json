{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "alarms"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
